{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "strictNullChecks": true,
    // "strict": true,
    "paths": {
      "src/*": ["src/*"]
    },
    "jsx": "react-native",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    // "skipLibCheck":true,
    "allowJs": true,
    "outDir": "dist",
    "declaration": true
  }
}
